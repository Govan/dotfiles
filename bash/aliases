# changing directory to code project
function p { cd ~/Projects/$1; }

alias bash-vi?="less ~/.bash/vi-mode-cheatsheet"

alias k="clear"

alias ls="ls --color"
if [ -f mvim ]
then
 alias vim="mvim"
fi
# force tmux to start with 256 color support
alias tmux="tmux -2"

# If tmuxis running attach to it,otherwise start a new sessssion
alias ltmux="if tmux has; then tmux attach; else tmux new; fi"

# Use Quicklook from the Terminal
function ql { qlmanage -p $1 > /dev/null; }

